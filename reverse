import java.util.*;
class reverse
{
 public static void main(String args[])
 {
  Scanner sc=new Scanner(System.in);
  String s=sc.nextLine();
  StringBuffer sb=new StringBuffer(s);
  sb.reverse(s);
  if(s==sb)
  {
   System.out.println("equal");
   }
   else
   {
   System.out.println("not equal");
  }
}
}
